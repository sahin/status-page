{
  "name": "status_page_parser",
  "version": "0.0.1",
  "description": "Status Page Parse",
  "author": "Sahin <sahinboydas@gmail.com>",
  "scripts": {
    "build:clean": "rimraf dist",
    "build:babel": "babel src -d dist",
    "build": "npm run build:clean && npm run build:babel",
    "start-dev:babel-node": "babel-node src/index.js",
    "start-dev:babel-node:debug": "babel-node --debug src/index.js",
    "start-dev:babel-node:debug-brk": "babel-node --debug-brk src/index.js",
    "start-dev": "nodemon --exec npm run start-dev:babel-node --watch src",
    "start-dev-debug-brk": "nodemon --exec npm run start-dev:babel-node:debug-brk --watch src",
    "start-dev-debug": "nodemon --exec npm run start-dev:babel-node:debug --watch src",
    "start": "babel-node src/index.js"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-eslint": "6.0.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "chai": "3.5.0",
    "eslint": "2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.3.0",
    "knex": "0.10.0",
    "lokka": "1.7.0",
    "lokka-transport-http": "1.3.2",
    "mocha": "2.4.5",
    "nodemon": "1.9.1",
    "rewire": "2.5.1",
    "supertest": "1.2.0"
  },
  "dependencies": {
    "async": "^2.0.0-rc.4",
    "aws-sdk": "latest",
    "babel-cli": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.7.5",
    "colors": "^1.1.2",
    "github": "^0.2.4",
    "github-api": "^0.11.2",
    "http": "0.0.0",
    "jsdom": "^8.5.0",
    "merge": "^1.2.0",
    "querystring": "^0.2.0",
    "scrape-it": "^2.2.0",
    "string": "^3.3.1",
    "url": "^0.11.0"
  }
}
